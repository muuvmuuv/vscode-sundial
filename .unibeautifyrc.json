{
  "TypeScript": {
    "beautifiers": ["Prettier"],
    "TSLint": {
      "prefer_beautifier_config": true
    }
  },
  "JSON": {
    "beautifiers": ["Pretty Diff"],
    "indent_size": "2",
    "indent_style": "space",
    "quotes": "double",
    "max_preserve_newlines": 0
  },
  "Python": {
    "beautifiers": ["Black"],
    "wrap_line_length": 80
  },
  "Markdown": {
    "beautifiers": ["Prettier"],
    "wrap_line_length": 80,
    "wrap_prose": "always"
  }
}
