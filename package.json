{
  "name": "vscode-sundial",
  "displayName": "Sundial â€“ Day/Night Theme Switcher (auto Dark mode)",
  "version": "1.17.0",
  "description": "Change your VS Code theme/settings based on your sunset, sunrise, system appearance or other preferences!",
  "icon": "assets/icon.jpg",
  "publisher": "muuvmuuv",
  "qna": false,
  "keywords": [
    "day",
    "night",
    "sundial",
    "sunrise",
    "sunset",
    "switch",
    "theme",
    "vscode"
  ],
  "homepage": "https://github.com/muuvmuuv/vscode-sundial",
  "bugs": "https://github.com/muuvmuuv/vscode-sundial/issues",
  "repository": "https://github.com/muuvmuuv/vscode-sundial",
  "license": "MIT",
  "author": {
    "name": "Marvin Heilemann",
    "email": "marvin.heilemann+github@googlemail.com",
    "url": "https://marvin.digital/"
  },
  "main": "./dist/extension.js",
  "scripts": {
    "start": "npm run watch",
    "dev": "webpack --mode development --progress",
    "prod": "webpack --mode production --progress --env.analyze",
    "watch": "webpack --mode development --watch",
    "============= VS Code =============": "",
    "vscode:prepublish": "webpack --mode production",
    "============= Release =============": "",
    "release": "node ./scripts/release.js",
    "changelog": "auto-changelog",
    "package": "vsce package",
    "============= Scripts =============": "",
    "version": "node ./scripts/version.js",
    "============== Tests =============": "",
    "lint": "tslint -p ./",
    "pretest": "npm run compile",
    "test": "echo \"Currently not supported\"",
    "test-off": "node ./out/test/runTest.js",
    "compile": "tsc -p ./",
    "": ""
  },
  "dependencies": {
    "dark-mode": "^3.0.0",
    "dayjs": "^1.8.17",
    "got": "^10.0.1",
    "loglevel": "^1.6.6",
    "public-ip": "^4.0.0",
    "suncalc": "^1.8.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/got": "^9.6.9",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.12.14",
    "@types/semver": "^6.2.0",
    "@types/suncalc": "^1.8.0",
    "@types/vscode": "1.25.0",
    "auto-changelog": "^1.16.2",
    "clean-webpack-plugin": "^3.0.0",
    "execa": "^3.4.0",
    "glob": "^7.1.6",
    "kleur": "^3.0.3",
    "mocha": "^6.2.2",
    "prompts": "^2.3.0",
    "release-it": "^12.4.3",
    "semver": "^6.3.0",
    "source-map-support": "^0.5.16",
    "terser-webpack-plugin": "^2.2.1",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "vscode-test": "^1.2.3",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10"
  },
  "engines": {
    "vscode": "^1.25.0"
  },
  "activationEvents": [
    "*"
  ],
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": false,
    "commitLimit": false
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "sundial.switchToNightTheme",
        "title": "Night Theme ðŸŒ‘"
      },
      {
        "command": "sundial.switchToDayTheme",
        "title": "Day Theme ðŸŒ•"
      },
      {
        "command": "sundial.toggleDayNightTheme",
        "title": "Toggle Day/Night Theme"
      },
      {
        "command": "sundial.continueAutomation",
        "title": "Continue switching day/night theme automatically"
      }
    ],
    "keybindings": [
      {
        "command": "sundial.toggleDayNightTheme",
        "key": "ctrl+alt+t",
        "mac": "ctrl+cmd+t"
      }
    ],
    "menus": {
      "touchBar": [
        {
          "command": "sundial.switchToNightTheme",
          "group": "editing"
        },
        {
          "command": "sundial.switchToDayTheme",
          "group": "editing"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Sundial Configurations",
      "properties": {
        "sundial.dayTheme": {
          "type": "string",
          "default": "Default Light+",
          "description": "Choose a theme to display on the day."
        },
        "sundial.nightTheme": {
          "type": "string",
          "default": "Default Dark+",
          "description": "Choose a theme to display on the night."
        },
        "sundial.sunrise": {
          "type": "string",
          "default": "07:00",
          "description": "Set the sunrise time. (24 hour format)"
        },
        "sundial.sunset": {
          "type": "string",
          "default": "19:00",
          "description": "Set the sunset time. (24 hour format)"
        },
        "sundial.latitude": {
          "type": "string",
          "description": "Latitude of your location to get sunset and sunrise."
        },
        "sundial.longitude": {
          "type": "string",
          "description": "Latitude of your location to get sunset and sunrise."
        },
        "sundial.autoLocale": {
          "type": "boolean",
          "default": false,
          "description": "Automatically set your sunset and sunrise based on your public ip location."
        },
        "sundial.systemTheme": {
          "type": "boolean",
          "default": false,
          "description": "Use your current operating system theme."
        },
        "sundial.ambientLight": {
          "type": "boolean",
          "default": false,
          "description": "Use your computers ambient light sensor to check if dark mode is needed."
        },
        "sundial.dayVariable": {
          "type": "number",
          "default": 0,
          "description": "Set a variable to change the theme X minutes before or after sunrise."
        },
        "sundial.nightVariable": {
          "type": "number",
          "default": 0,
          "description": "Set a variable to change the theme X minutes before or after sunset."
        },
        "sundial.daySettings": {
          "type": "object",
          "default": {},
          "description": "Set some VSCode settings you would like to override during the day."
        },
        "sundial.nightSettings": {
          "type": "object",
          "default": {},
          "description": "Set some VSCode settings you would like to override during the night."
        },
        "sundial.interval": {
          "type": "number",
          "default": 5,
          "description": "Set in which interval Sundial should check the time. (in minutes)"
        },
        "sundial.debug": {
          "type": "number",
          "default": 2,
          "description": "More verbose console logs. See: https://github.com/pimterry/loglevel/blob/master/lib/loglevel.js#L181"
        },
        "sundial.windowEvents": {
          "type": "array",
          "default": [
            "onDidChangeWindowState",
            "onDidChangeActiveTextEditor",
            "onDidChangeTextEditorViewColumn"
          ],
          "description": "Specify on which VS Code window events you want Sundial to check."
        },
        "sundial.workspaceEvents": {
          "type": "array",
          "default": [
            "onDidChangeConfiguration"
          ],
          "description": "Specify on which VS Code workspace events you want Sundial to check."
        }
      }
    }
  }
}
