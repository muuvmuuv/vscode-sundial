{
  "name": "vscode-sundial",
  "displayName": "Sundial",
  "version": "1.5.0",
  "icon": "assets/icon.jpg",
  "publisher": "muuvmuuv",
  "description": "Change your VS Code theme based on your sunset and sunrise!",
  "keywords": [
    "theme",
    "switch",
    "day",
    "night",
    "sunrise",
    "sunset",
    "sundial",
    "moment",
    "vscode"
  ],
  "categories": [
    "Other"
  ],
  "author": {
    "name": "Marvin Heilemann",
    "email": "marvin.heilemann+github@googlemail.com",
    "url": "https://marvin.digital/"
  },
  "homepage": "https://github.com/muuvmuuv/vscode-sundial/blob/master/README.md",
  "bugs": "https://github.com/muuvmuuv/vscode-sundial/issues",
  "qna": false,
  "repository": "https://github.com/muuvmuuv/vscode-sundial",
  "license": "MIT",
  "main": "./dist/extension",
  "scripts": {
    "pretty": "pretty-quick",
    "lint": "tslint src/*.ts",
    "vscode:prepublish": "webpack --mode production",
    "prod": "webpack --mode production --progress",
    "dev": "webpack --mode development --progress",
    "watch": "webpack --mode development --watch",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "version": "ts-node -T ./scripts/version.ts",
    "tag": "run-s tag:check tag:create tag:push",
    "tag:check": "ts-node -T ./scripts/tag.ts",
    "tag:create": "git tag -a v$npm_package_version -m \"New version: ${npm_package_version}\"",
    "tag:push": "git push origin v${npm_package_version}"
  },
  "engines": {
    "vscode": "^1.25.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "got": "^9.6.0",
    "moment": "^2.24.0",
    "public-ip": "^3.0.0",
    "suncalc": "^1.8.0"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/got": "^9.4.3",
    "@types/inquirer": "^6.0.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.6",
    "@types/public-ip": "^2.4.0",
    "@types/semver": "^6.0.0",
    "@types/suncalc": "^1.8.0",
    "@types/terser-webpack-plugin": "^1.2.1",
    "@types/webpack": "^4.4.27",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^2.0.1",
    "execa": "^1.0.0",
    "husky": "^1.3.1",
    "inquirer": "^6.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "semver": "^6.0.0",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.4",
    "vscode": "^1.1.33",
    "webpack": "^4.30.0",
    "webpack-build-notifier": "^1.0.1",
    "webpack-cli": "^3.3.0"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "sundial.switchToNightTheme",
        "title": "Sundial: Night Theme"
      },
      {
        "command": "sundial.switchToDayTheme",
        "title": "Sundial: Day Theme"
      },
      {
        "command": "sundial.toggleDayNightTheme",
        "title": "Sundial: Toggle Day/Night Theme"
      },
      {
        "command": "sundial.continueAutomation",
        "title": "Sundial: Continue switching day/night theme automatically"
      }
    ],
    "keybindings": [
      {
        "command": "sundial.toggleDayNightTheme",
        "key": "ctrl+alt+t",
        "mac": "ctrl+cmd+t"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Sundial Configurations",
      "properties": {
        "sundial.dayTheme": {
          "type": "string",
          "default": "Default Light+",
          "description": "Choose a theme to display on the day."
        },
        "sundial.nightTheme": {
          "type": "string",
          "default": "Default Dark+",
          "description": "Choose a theme to display on the night."
        },
        "sundial.sunrise": {
          "type": "string",
          "default": "07:00",
          "description": "Set the sunrise time. (24 hour format)"
        },
        "sundial.sunset": {
          "type": "string",
          "default": "19:00",
          "description": "Set the sunset time. (24 hour format)"
        },
        "sundial.latitude": {
          "type": "string",
          "default": null,
          "description": "Latitude of your location to get sunset and sunrise."
        },
        "sundial.longitude": {
          "type": "string",
          "default": null,
          "description": "Latitude of your location to get sunset and sunrise."
        },
        "sundial.autoLocale": {
          "type": "boolean",
          "default": false,
          "description": "Automatically set your sunset and sunrise based on your public ip location."
        },
        "sundial.dayVariable": {
          "type": "number",
          "default": 0,
          "description": "Set a variable to change the theme X minutes before or after sunrise."
        },
        "sundial.nightVariable": {
          "type": "number",
          "default": 0,
          "description": "Set a variable to change the theme X minutes before or after sunset."
        },
        "sundial.interval": {
          "type": "number",
          "default": 5,
          "description": "Set in which interval Sundial should check the time. (in minutes)"
        },
        "sundial.useHTTPS": {
          "type": "boolean",
          "default": false,
          "description": "Use some services with HTTPS instead of HTTP."
        },
        "sundial.debug": {
          "type": "boolean",
          "default": false,
          "description": "Prints some outputs while developing."
        }
      }
    }
  }
}
