{
  "name": "vscode-sundial",
  "displayName": "Sundial",
  "version": "1.10.0",
  "icon": "assets/icon.jpg",
  "publisher": "muuvmuuv",
  "description": "Change your VS Code theme based on your sunset and sunrise!",
  "enableProposedApi": false,
  "keywords": [
    "theme",
    "switch",
    "day",
    "night",
    "sunrise",
    "sunset",
    "sundial",
    "moment",
    "vscode"
  ],
  "categories": [
    "Other"
  ],
  "author": {
    "name": "Marvin Heilemann",
    "email": "marvin.heilemann+github@googlemail.com",
    "url": "https://marvin.digital/"
  },
  "homepage": "https://github.com/muuvmuuv/vscode-sundial/blob/master/README.md",
  "bugs": "https://github.com/muuvmuuv/vscode-sundial/issues",
  "qna": false,
  "repository": "https://github.com/muuvmuuv/vscode-sundial",
  "license": "MIT",
  "main": "./dist/extension.js",
  "scripts": {
    "pretty": "pretty-quick",
    "lint": "tslint src/*.ts",
    "vscode:prepublish": "webpack --mode production",
    "prod": "webpack --mode production --progress",
    "dev": "webpack --mode development --progress",
    "watch": "webpack --mode development --watch",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "changelog": "auto-changelog",
    "release": "ts-node -T ./scripts/release.ts",
    "version": "ts-node -T ./scripts/version.ts",
    "test": "npm run test-compile",
    "test-compile": "tsc -p ./"
  },
  "engines": {
    "vscode": "^1.25.0"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": false,
    "commitLimit": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "dark-mode": "^3.0.0",
    "got": "^9.6.0",
    "loglevel": "^1.6.3",
    "moment": "^2.24.0",
    "suncalc": "^1.8.0"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^7.0.0",
    "@types/got": "^9.4.4",
    "@types/inquirer": "^6.0.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.8",
    "@types/public-ip": "^2.4.0",
    "@types/semver": "^6.0.0",
    "@types/suncalc": "^1.8.0",
    "@types/terser-webpack-plugin": "^1.2.1",
    "@types/webpack": "^4.4.32",
    "auto-changelog": "^1.13.0",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^3.0.0",
    "execa": "^1.0.0",
    "fs-extra": "^8.0.1",
    "husky": "^2.4.1",
    "inquirer": "^6.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0",
    "release-it": "^12.3.0",
    "semver": "^6.1.1",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2",
    "vscode": "^1.1.34",
    "webpack": "^4.34.0",
    "webpack-build-notifier": "^1.0.3",
    "webpack-cli": "^3.3.4"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "sundial.switchToNightTheme",
        "title": "Sundial: Night Theme"
      },
      {
        "command": "sundial.switchToDayTheme",
        "title": "Sundial: Day Theme"
      },
      {
        "command": "sundial.toggleDayNightTheme",
        "title": "Sundial: Toggle Day/Night Theme"
      },
      {
        "command": "sundial.continueAutomation",
        "title": "Sundial: Continue switching day/night theme automatically"
      }
    ],
    "keybindings": [
      {
        "command": "sundial.toggleDayNightTheme",
        "key": "ctrl+alt+t",
        "mac": "ctrl+cmd+t"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Sundial Configurations",
      "properties": {
        "sundial.dayTheme": {
          "type": "string",
          "default": "Default Light+",
          "description": "Choose a theme to display on the day."
        },
        "sundial.nightTheme": {
          "type": "string",
          "default": "Default Dark+",
          "description": "Choose a theme to display on the night."
        },
        "sundial.sunrise": {
          "type": "string",
          "default": "07:00",
          "description": "Set the sunrise time. (24 hour format)"
        },
        "sundial.sunset": {
          "type": "string",
          "default": "19:00",
          "description": "Set the sunset time. (24 hour format)"
        },
        "sundial.latitude": {
          "type": "string",
          "description": "Latitude of your location to get sunset and sunrise."
        },
        "sundial.longitude": {
          "type": "string",
          "description": "Latitude of your location to get sunset and sunrise."
        },
        "sundial.autoLocale": {
          "type": "boolean",
          "default": false,
          "description": "Automatically set your sunset and sunrise based on your public ip location."
        },
        "sundial.systemTheme": {
          "type": "boolean",
          "default": false,
          "description": "Use your current operating system theme."
        },
        "sundial.ambientLight": {
          "type": "boolean",
          "default": false,
          "description": "Use your computers ambient light sensor to check if dark mode is needed."
        },
        "sundial.dayVariable": {
          "type": "number",
          "default": 0,
          "description": "Set a variable to change the theme X minutes before or after sunrise."
        },
        "sundial.nightVariable": {
          "type": "number",
          "default": 0,
          "description": "Set a variable to change the theme X minutes before or after sunset."
        },
        "sundial.daySettings": {
          "type": "object",
          "default": {},
          "description": "Set some VSCode settings you would like to override during the day."
        },
        "sundial.nightSettings": {
          "type": "object",
          "default": {},
          "description": "Set some VSCode settings you would like to override during the night."
        },
        "sundial.interval": {
          "type": "number",
          "default": 5,
          "description": "Set in which interval Sundial should check the time. (in minutes)"
        },
        "sundial.debug": {
          "type": "boolean",
          "default": false,
          "description": "More verbose console logs."
        },
        "sundial.windowEvents": {
          "type": "array",
          "default": [
            "onDidChangeWindowState",
            "onDidChangeActiveTextEditor",
            "onDidChangeTextEditorViewColumn"
          ],
          "description": "Specify on which VS Code window events you want Sundial to check."
        },
        "sundial.workspaceEvents": {
          "type": "array",
          "default": [
            "onDidChangeConfiguration",
            "onDidChangeTextDocument"
          ],
          "description": "Specify on which VS Code workspace events you want Sundial to check."
        }
      }
    }
  }
}
