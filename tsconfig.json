{
  "compilerOptions": {
    "rootDir": "src",
    "module": "CommonJS",
    "target": "ES6",
    "outDir": "out",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "lib": [
      "ES6"
    ]
  },
  "exclude": [
    "node_modules",
    ".vscode-test"
  ]
}